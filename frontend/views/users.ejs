<section>
  <h2>User Management</h2>
  <% if (error) { %>
    <p>Error: <%= error %></p>
  <% } else if (users && users.length > 0) { %>
    <ul>
      <% users.forEach(user => { %>
        <li><%= user.name %> - <%= user.email %></li>
      <% }); %>
    </ul>
  <% } else { %>
    <p>No users found.</p>
  <% } %>
</section>